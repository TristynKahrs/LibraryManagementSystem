<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewscontrollers.UserProfileController">
   <children>
      <ImageView fx:id="imgUser" fitHeight="50.0" fitWidth="50.0" layoutX="29.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="lblGreeting" layoutX="65.0" layoutY="17.0" prefHeight="50.0" prefWidth="445.0" text="Hey">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Pane fx:id="paneDisplay" layoutX="2.0" layoutY="79.0" prefHeight="230.0" prefWidth="600.0" />
      <HBox alignment="CENTER" layoutY="359.0" prefHeight="41.0" prefWidth="604.0">
         <children>
            <Button fx:id="btnFees" maxWidth="1.7976931348623157E308" minWidth="-Infinity" mnemonicParsing="false" onAction="#onClickFees" text="Fees">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btnLostBooks" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onClickLostBooks" text="Lost Books">
