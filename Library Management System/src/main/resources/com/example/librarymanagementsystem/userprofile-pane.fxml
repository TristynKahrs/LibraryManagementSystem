            </Button>
            <Button fx:id="btnChangePassword" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onClickChangePassword" text="Change Password">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btnCheckedOutBooks" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onClickCheckedOut" text="Checked out Books">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <Button fx:id="btnLogout" layoutX="530.0" layoutY="14.0" mnemonicParsing="false" onAction="#logoutOnClick" text="Log Out" />
      <Button fx:id="btnBrowse" layoutX="532.0" layoutY="38.0" mnemonicParsing="false" onAction="#onClickBrowse" text="Browse" />
   </children>
</AnchorPane>
