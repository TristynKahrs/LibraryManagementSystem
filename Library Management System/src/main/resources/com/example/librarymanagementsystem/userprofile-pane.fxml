<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewscontrollers.UserProfileController">
   <children>
      <ImageView fx:id="imgUser" fitHeight="50.0" fitWidth="50.0" layoutX="29.0" layoutY="14.0" onMouseClicked="#switchMenu" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/defaultpfp.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ImageView>
      <Label fx:id="lblGreeting" layoutX="87.0" layoutY="14.0" prefHeight="50.0" prefWidth="445.0" text="Hey">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Pane fx:id="paneDisplay" layoutX="2.0" layoutY="79.0" prefHeight="319.0" prefWidth="600.0" />
      <Button fx:id="btnBrowse" layoutX="533.0" layoutY="26.0" mnemonicParsing="false" onAction="#onClickBrowse" text="Browse" />
   </children>
</AnchorPane>
