<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane fx:id="paneCreateAccount" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewscontrollers.CreateAccountController">
   <children>
      <Label layoutX="192.0" layoutY="34.0" text="Create Account">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <TextField fx:id="txtFullName" layoutX="226.0" layoutY="99.0" promptText="Full name" />
      <TextField fx:id="txtUsername" layoutX="226.0" layoutY="147.0" promptText="Username" />
      <TextField fx:id="txtPassword" layoutX="226.0" layoutY="200.0" promptText="Password" />
      <TextField fx:id="txtPasswordConfirmation" layoutX="226.0" layoutY="250.0" promptText="Confirm Password" />
      <Button fx:id="btnCreate" layoutX="251.0" layoutY="300.0" mnemonicParsing="false" onAction="#onCreateAccountBtn" text="Create Account" />
   </children>
</Pane>
