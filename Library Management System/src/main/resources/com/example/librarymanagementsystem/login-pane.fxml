<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane fx:id="paneLogin" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="400.0" minWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewscontrollers.LoginController">
    <children>
           <Label alignment="CENTER" contentDisplay="CENTER" layoutX="228.0" layoutY="17.0" prefHeight="34.0" prefWidth="144.0" text="Welcome!" textAlignment="CENTER">
               <font>
                   <Font size="32.0" />
               </font>
           </Label>
           <TextField fx:id="txtUsername" layoutX="226.0" layoutY="92.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="149.0" promptText="Username" />
           <PasswordField fx:id="txtPassword" layoutX="226.0" layoutY="136.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="149.0" promptText="Password" />
           <Button fx:id="btnLogin" layoutX="275.0" layoutY="188.0" mnemonicParsing="false" onAction="#onClickLogin" text="Log In">
               <cursor>
                   <Cursor fx:constant="HAND" />
               </cursor>
           </Button>
           <Label id="hyperlink" fx:id="lblCreateAccount" layoutX="258.0" layoutY="321.0" onMouseClicked="#onCreateAccountLink" text="Create Account" underline="true">
               <cursor>
                   <Cursor fx:constant="HAND" />
               </cursor>
           </Label>
           <Label id="hyperlink" fx:id="lblForgotPassword" layoutX="255.0" layoutY="350.0" onMouseClicked="#onForgotPasswordClick" prefHeight="18.0" text="Forgot Password" underline="true">
               <cursor>
                   <Cursor fx:constant="HAND" />
               </cursor>
           </Label>
    </children>
</Pane>
