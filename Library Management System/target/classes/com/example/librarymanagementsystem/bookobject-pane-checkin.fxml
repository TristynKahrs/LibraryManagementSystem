<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="checkInPane" prefHeight="90.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fx:id="imgBook" fitHeight="69.0" fitWidth="74.0" layoutX="22.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true" />
        <Label fx:id="lblTitle" layoutX="119.0" layoutY="14.0">
            <font>
                <Font size="14.0" />
            </font></Label>
        <Label fx:id="lblAuthor" layoutX="119.0" layoutY="56.0" text=" ">
            <font>
                <Font size="14.0" />
            </font></Label>
      <Button fx:id="btnCheckIn" layoutX="269.0" layoutY="11.0" mnemonicParsing="false" onAction="#onCheckInClick" text="Check In" />
      <Button fx:id="btnLost" layoutX="261.0" layoutY="53.0" mnemonicParsing="false" onAction="#onReportLostClick" text="Report Lost" />
    </children>
   <padding>
      <Insets bottom="3.0" left="3.0" top="3.0" />
   </padding>
</AnchorPane>
