<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="230.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewscontrollers.ChangePasswordPaneController">
    <children>
        <TextField fx:id="txtCurrentPassword" layoutX="202.0" layoutY="60.0" prefHeight="33.0" prefWidth="196.0" promptText="Current Password">
            <font>
                <Font size="14.0" />
            </font>
        </TextField>
        <TextField fx:id="txtNewPassword" layoutX="202.0" layoutY="101.0" prefHeight="33.0" prefWidth="196.0" promptText="New Password">
            <font>
                <Font size="14.0" />
            </font>
        </TextField>
        <TextField fx:id="txtConfirmPassword" layoutX="202.0" layoutY="140.0" prefHeight="33.0" prefWidth="196.0" promptText="Confirm New Password">
            <font>
                <Font size="14.0" />
            </font>
        </TextField>
        <Label layoutX="205.0" layoutY="14.0" text="Change Password">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Button fx:id="btnSubmit" layoutX="266.0" layoutY="181.0" mnemonicParsing="false" onAction="#onSubmitClick" prefHeight="33.0" prefWidth="68.0" text="Submit" />
        <Label fx:id="lblConfirmation" layoutX="250.0" layoutY="228.0" />
    </children>

</AnchorPane>
