            <Cursor fx:constant="HAND" />
         </cursor></Label>
      <Label id="hyperlink" fx:id="lblLogout" layoutX="14.0" layoutY="269.0" onMouseClicked="#onLogout" text="Logout" underline="true">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Label>
      <Label id="hyperlink" fx:id="lblThemes" layoutX="14.0" layoutY="245.0" onMouseClicked="#onThemes" text="Toggle Theme" underline="true" wrapText="true">
